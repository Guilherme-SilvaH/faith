{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "src/**/*.ts",  // Inclui todos os arquivos TypeScript no diretório `src`
    "src/@types/**/*.d.ts"  // Explicitamente inclui os arquivos de definições de tipo
    , "Controller"]
}

